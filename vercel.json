{
    "rewrites": [
      {
        "source": "/kalshi/:path*",
        "destination": "https://api.elections.kalshi.com/trade-api/v2/:path*"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS" }
        ]
      }
    ]
  }